{
  "name": "bastion-siege-assist-bot",
  "private": "true",
  "version": "2.9.1",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EdJoPaTo/BastionSiegeAssistBot.git"
  },
  "bugs": {
    "url": "https://github.com/EdJoPaTo/BastionSiegeAssistBot/issues"
  },
  "homepage": "https://github.com/EdJoPaTo/BastionSiegeAssistBot#readme",
  "main": "index.js",
  "engines": {
    "node": ">=11"
  },
  "scripts": {
    "ava": "ava",
    "ava:watch": "ava -w",
    "nyc": "nyc",
    "start": "node index.js",
    "pretest": "xo",
    "test": "nyc ava",
    "xo": "xo"
  },
  "config": {
    "tokenpath": "/run/secrets/bastionsiegeassist-token.txt",
    "tokenpathdebug": "token.txt"
  },
  "dependencies": {
    "array-filter-unique": "^2.0.0",
    "debounce-promise": "^3.1.0",
    "emoji-tree": "^1.1.2",
    "json-stable-stringify": "^1.0.1",
    "telegraf": "^3.26.0",
    "telegraf-inline-menu": "^3.2.2",
    "telegraf-session-local": "0.0.6"
  },
  "devDependencies": {
    "ava": "^1.1.0",
    "nyc": "^13.1.0",
    "xo": "^0.24.0"
  },
  "ava": {
    "modules": false
  },
  "xo": {
    "space": true,
    "semicolon": false,
    "overrides": [
      {
        "files": [
          "parts/inline-query.js"
        ],
        "rules": {
          "camelcase": [
            "warn",
            {
              "properties": "never"
            }
          ]
        }
      }
    ]
  }
}
